{
    "model": {
      "type": "minecraft:select",
      "property": "minecraft:custom_model_data",
      "cases": [
        {
          "when": "blue",
          "model": {
            "type": "minecraft:model",
            "model": "expansion:items/portal_cores/blue"
          }
        },
        {
            "when": "green",
            "model": {
              "type": "minecraft:model",
              "model": "expansion:items/portal_cores/green"
            }
        },
        {
            "when": "purple",
            "model": {
              "type": "minecraft:model",
              "model": "expansion:items/portal_cores/purple"
            }
        },
        {
            "when": "red",
            "model": {
              "type": "minecraft:model",
              "model": "expansion:items/portal_cores/red"
            }
        },
        {
            "when": "yellow",
            "model": {
              "type": "minecraft:model",
              "model": "expansion:items/portal_cores/yellow"
            }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "expansion:items/portal_cores/blue"
      }
    }
  }