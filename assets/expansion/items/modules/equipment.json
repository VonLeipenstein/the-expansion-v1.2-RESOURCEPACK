{
    "model": {
      "type": "minecraft:select",
      "property": "minecraft:custom_model_data",
      "cases": [
        {
          "when": "armored",
          "model": {
            "type": "minecraft:model",
            "model": "expansion:items/modules/equipment/armored"
          }
        },
        {
          "when": "cooling",
          "model": {
            "type": "minecraft:model",
            "model": "expansion:items/modules/equipment/cooling"
          }
        },
        {
          "when": "mobility",
          "model": {
            "type": "minecraft:model",
            "model": "expansion:items/modules/equipment/mobility"
          }
        },
        {
          "when": "oxygen",
          "model": {
            "type": "minecraft:model",
            "model": "expansion:items/modules/equipment/oxygen"
          }
        },
        {
          "when": "warming",
          "model": {
            "type": "minecraft:model",
            "model": "expansion:items/modules/equipment/warming"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "expansion:items/modules/equipment/oxygen"
      }
    }
  }